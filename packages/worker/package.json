{
  "name": "@expo/worker",
  "version": "0.1.70",
  "private": true,
  "scripts": {
    "start": "yarn watch",
    "watch": "yarn compile --watch --preserveWatchOutput",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "clean-all": "yarn clean && rm -rf node_modules",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "yarn test:unit",
    "test:unit": "./resources/jest --config jest.config.unit.ts",
    "test:unit:watch": "./resources/jest --config jest.config.unit.ts --watch",
    "test:integration": "./resources/jest --runInBand --config jest.config.integration.js",
    "test:integration:watch": "./resources/jest --runInBand --config jest.config.integration.js --watchAll",
    "typecheck": "tsc"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@babel/core": "7.24.7",
    "@expo/build-tools": "1.0.271",
    "@expo/config-plugins": "8.0.8",
    "@expo/config-types": "52.0.1",
    "@expo/downloader": "1.0.260",
    "@expo/eas-build-job": "1.0.265",
    "@expo/logger": "1.0.260",
    "@expo/results": "1.0.0",
    "@expo/rudder-sdk-node": "1.1.1",
    "@expo/spawn-async": "1.7.2",
    "@expo/steps": "1.0.271",
    "@expo/template-file": "1.0.260",
    "@expo/turtle-spawn": "1.0.260",
    "@hapi/boom": "10.0.1",
    "@sentry/node": "7.77.0",
    "check-disk-space": "3.4.0",
    "fast-glob": "3.3.2",
    "filesize": "7.0.0",
    "flatted": "3.2.5",
    "fs-extra": "11.2.0",
    "koa": "3.1.1",
    "koa-body": "7.0.1",
    "koa-router": "14.0.0",
    "lodash": "4.17.21",
    "micromatch": "4.0.8",
    "node-fetch": "2.7.0",
    "node-os-utils": "1.3.7",
    "nullthrows": "1.1.1",
    "semver": "7.6.3",
    "tar": "6.2.1",
    "uuid": "3.4.0",
    "ws": "8.18.0",
    "xml2js": "0.6.2",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@types/babel__core": "7.20.5",
    "@types/fs-extra": "11.0.4",
    "@types/koa": "3.0.1",
    "@types/koa-router": "7.4.9",
    "@types/lodash": "4.17.4",
    "@types/micromatch": "4.0.7",
    "@types/node": "20.14.8",
    "@types/node-fetch": "2.6.7",
    "@types/node-os-utils": "1.3.0",
    "@types/semver": "7.5.8",
    "@types/supertest": "6.0.3",
    "@types/tar": "6.1.10",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.10",
    "jest": "29.7.0",
    "memfs": "3.4.1",
    "supertest": "7.2.2",
    "ts-jest": "29.1.4",
    "typescript": "5.5.4"
  },
  "volta": {
    "node": "20.18.3",
    "yarn": "1.22.22"
  }
}
