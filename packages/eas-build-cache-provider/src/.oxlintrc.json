{
  "extends": ["../.oxlintrc.json"],
  "jsPlugins": [
    {
      "name": "async-protect",
      "specifier": "eslint-plugin-async-protect"
    },
    {
      "name": "eslint-import",
      "specifier": "eslint-plugin-import"
    }
  ],
  "rules": {
    "typescript/explicit-function-return-type": [
      "warn",
      {
        "allowExpressions": true
      }
    ]
  }
}
