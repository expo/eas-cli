{
  "name": "eas-cli",
  "description": "EAS command line tool",
  "version": "0.24.1",
  "author": "Expo <support@expo.dev>",
  "bin": {
    "eas": "./bin/run"
  },
  "bugs": "https://github.com/expo/eas-cli/issues",
  "dependencies": {
    "@amplitude/identify": "1.7.0",
    "@amplitude/node": "1.7.0",
    "@expo/apple-utils": "0.0.0-alpha.25",
    "@expo/config": "3.3.42",
    "@expo/config-plugins": "3.0.3",
    "@expo/eas-build-job": "0.2.46",
    "@expo/eas-json": "^0.24.0",
    "@expo/json-file": "8.2.25",
    "@expo/pkcs12": "0.0.4",
    "@expo/plist": "0.0.14",
    "@expo/plugin-warn-if-update-available": "1.7.0",
    "@expo/results": "1.0.0",
    "@expo/rudder-sdk-node": "1.0.7",
    "@expo/sdk-runtime-versions": "1.0.0",
    "@expo/spawn-async": "1.5.0",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.5",
    "@oclif/plugin-help": "3.2.3",
    "@urql/core": "2.3.1",
    "@urql/exchange-retry": "0.3.0",
    "chalk": "4.1.2",
    "cli-table3": "0.6.0",
    "dateformat": "4.5.1",
    "env-paths": "2.2.0",
    "fast-glob": "3.2.7",
    "figures": "3.2.0",
    "form-data": "3.0.0",
    "fs-extra": "10.0.0",
    "getenv": "1.0.0",
    "got": "11.8.2",
    "graphql": "15.5.1",
    "graphql-tag": "2.12.5",
    "ignore": "5.1.8",
    "indent-string": "4.0.0",
    "jks-js": "1.0.1",
    "joi": "17.4.2",
    "keychain": "1.3.0",
    "lodash": "4.17.21",
    "mime": "2.5.2",
    "minimatch": "3.0.4",
    "nanoid": "3.1.25",
    "node-fetch": "2.6.1",
    "node-forge": "0.10.0",
    "nullthrows": "1.1.1",
    "ora": "5.1.0",
    "pkg-dir": "4.2.0",
    "prompts": "2.3.2",
    "qrcode-terminal": "0.12.0",
    "resolve-from": "5.0.0",
    "semver": "7.3.5",
    "strip-ansi": "6.0.0",
    "tar": "6.1.11",
    "tempy": "0.7.0",
    "terminal-link": "2.1.1",
    "timeago.js": "4.0.2",
    "tslib": "2.3.1",
    "turndown": "7.1.1",
    "untildify": "4.0.0",
    "uuid": "8.3.2",
    "wrap-ansi": "7.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@types/dateformat": "3.0.1",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "9.0.12",
    "@types/getenv": "^1.0.0",
    "@types/lodash": "4.14.172",
    "@types/mime": "2.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/node-forge": "^0.9.5",
    "@types/prompts": "^2.0.9",
    "@types/semver": "7.3.8",
    "@types/tar": "4.0.5",
    "@types/tough-cookie": "4.0.1",
    "@types/uuid": "8.3.1",
    "@types/wrap-ansi": "3.0.0",
    "axios": "0.21.1",
    "eslint-plugin-graphql": "4.0.0",
    "memfs": "3.2.2",
    "mockdate": "^3.0.2",
    "nock": "^13.0.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/build",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/expo/eas-cli",
  "keywords": [
    "expo",
    "eas",
    "cli"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "oclif": {
    "bin": "eas",
    "commands": "./build/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@expo/plugin-warn-if-update-available"
    ],
    "helpClass": "./build/help",
    "topics": {
      "account": {
        "description": "manage your account"
      },
      "build": {
        "description": "build app binaries"
      },
      "channel": {
        "description": "manage update channels"
      },
      "device": {
        "description": "manage your Apple devices for internal distribution"
      },
      "project": {
        "description": "manage your project"
      },
      "release": {
        "description": "manage update releases"
      },
      "secret": {
        "description": "manage project and account secrets"
      },
      "update": {
        "description": "manage individual updates"
      },
      "webhook": {
        "description": "manage webhooks"
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 0.5,
      "message": "<%= chalk.bold('â˜…') %> <%= chalk.bold(config.name + '@' + latest) %> <%= chalk.bold('is now available, please upgrade.') %>\n<%= chalk.dim('Proceeding with outdated version') %>\n"
    },
    "update": {
      "node": {
        "version": "12.13.0"
      },
      "s3": {
        "templates": {
          "target": {
            "unversioned": "<%- bin %>-<%- platform %>-<%- arch %><%- ext %>",
            "versioned": "<%- bin %>-<%- platform %>-<%- arch %><%- ext %>"
          }
        }
      }
    }
  },
  "repository": "expo/eas-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf build && yarn build && yarn oclif-dev manifest && yarn oclif-dev readme && node patch-readme",
    "build": "tsc --project tsconfig.build.json",
    "watch": "yarn build --watch --preserveWatchOutput",
    "typecheck": "tsc",
    "test": "jest",
    "version": "yarn oclif-dev readme && node patch-readme && git add README.md",
    "generate-graphql-code": "graphql-codegen --config graphql-codegen.yml"
  }
}
