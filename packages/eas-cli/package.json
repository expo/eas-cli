{
  "name": "eas-cli",
  "description": "EAS command line tool",
  "version": "0.44.1",
  "author": "Expo <support@expo.dev>",
  "bin": {
    "eas": "./bin/run"
  },
  "bugs": "https://github.com/expo/eas-cli/issues",
  "dependencies": {
    "@expo/apple-utils": "0.0.0-alpha.27",
    "@expo/config": "6.0.16",
    "@expo/config-plugins": "4.0.16",
    "@expo/eas-build-job": "0.2.63",
    "@expo/eas-json": "0.44.0",
    "@expo/json-file": "8.2.34",
    "@expo/pkcs12": "0.0.6",
    "@expo/plist": "0.0.17",
    "@expo/plugin-warn-if-update-available": "2.0.4",
    "@expo/prebuild-config": "3.0.16",
    "@expo/results": "1.0.0",
    "@expo/rudder-sdk-node": "1.1.1",
    "@expo/spawn-async": "1.5.0",
    "@expo/timeago.js": "1.0.0",
    "@oclif/core": "1.1.2",
    "@oclif/plugin-autocomplete": "1.1.1",
    "@oclif/plugin-help": "5.1.10",
    "@urql/core": "2.3.6",
    "@urql/exchange-retry": "0.3.2",
    "chalk": "4.1.2",
    "cli-table3": "0.6.1",
    "dateformat": "4.6.3",
    "env-paths": "2.2.0",
    "envinfo": "7.8.1",
    "fast-glob": "3.2.11",
    "figures": "3.2.0",
    "form-data": "4.0.0",
    "fs-extra": "10.0.0",
    "getenv": "1.0.0",
    "got": "11.8.2",
    "gradle-to-js": "2.0.0",
    "graphql": "16.2.0",
    "graphql-tag": "2.12.6",
    "ignore": "5.2.0",
    "indent-string": "4.0.0",
    "jks-js": "1.0.1",
    "joi": "17.5.0",
    "keychain": "1.3.0",
    "log-symbols": "4.1.0",
    "mime": "3.0.0",
    "minimatch": "3.0.4",
    "nanoid": "3.2.0",
    "node-fetch": "2.6.1",
    "node-forge": "1.2.1",
    "nullthrows": "1.1.1",
    "ora": "5.1.0",
    "pkg-dir": "4.2.0",
    "prompts": "2.4.2",
    "qrcode-terminal": "0.12.0",
    "resolve-from": "5.0.0",
    "semver": "7.3.5",
    "slash": "3.0.0",
    "strip-ansi": "6.0.0",
    "tar": "6.1.11",
    "terminal-link": "2.1.1",
    "tslib": "2.3.1",
    "turndown": "7.1.1",
    "untildify": "4.0.0",
    "uuid": "8.3.2",
    "wrap-ansi": "7.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@types/dateformat": "3.0.1",
    "@types/envinfo": "7.8.1",
    "@types/fs-extra": "9.0.13",
    "@types/getenv": "^1.0.0",
    "@types/mime": "2.0.3",
    "@types/node-fetch": "^2.5.7",
    "@types/node-forge": "1.0.0",
    "@types/prompts": "2.0.14",
    "@types/semver": "7.3.9",
    "@types/tar": "6.1.1",
    "@types/tough-cookie": "4.0.1",
    "@types/uuid": "8.3.4",
    "@types/wrap-ansi": "3.0.0",
    "axios": "0.24.0",
    "eslint-plugin-graphql": "4.0.0",
    "memfs": "3.4.1",
    "mockdate": "3.0.5",
    "nock": "13.2.2",
    "tempy": "0.7.0",
    "ts-deepmerge": "1.1.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/build",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/expo/eas-cli",
  "keywords": [
    "expo",
    "eas",
    "cli"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "eas",
    "commands": "./build/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@expo/plugin-warn-if-update-available"
    ],
    "topics": {
      "account": {
        "description": "manage your account"
      },
      "build": {
        "description": "build app binaries"
      },
      "channel": {
        "description": "manage update channels"
      },
      "device": {
        "description": "manage your Apple devices for internal distribution"
      },
      "project": {
        "description": "manage your project"
      },
      "release": {
        "description": "manage update releases"
      },
      "secret": {
        "description": "manage project and account secrets"
      },
      "update": {
        "description": "manage individual updates"
      },
      "webhook": {
        "description": "manage webhooks"
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 0.5,
      "message": "<%= chalk.bold('â˜…') %> <%= chalk.bold(config.name + '@' + latest) %> <%= chalk.bold('is now available, please upgrade.') %>\n<%= chalk.dim('Proceeding with outdated version') %>\n"
    },
    "update": {
      "node": {
        "version": "12.13.0"
      },
      "s3": {
        "templates": {
          "target": {
            "unversioned": "<%- bin %>-<%- platform %>-<%- arch %><%- ext %>",
            "versioned": "<%- bin %>-<%- platform %>-<%- arch %><%- ext %>"
          }
        }
      }
    },
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "expo/eas-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "./scripts/prepack.sh",
    "pretarball-ci": "./scripts/pretarball-ci.sh",
    "build": "tsc --project tsconfig.build.json",
    "watch": "yarn build --watch --preserveWatchOutput",
    "typecheck": "tsc",
    "test": "jest",
    "version": "yarn oclif readme && node scripts/patch-readme && git add README.md",
    "generate-graphql-code": "graphql-codegen --config graphql-codegen.yml",
    "clean": "rm -rf dist build tmp node_modules yarn-error.log"
  }
}
