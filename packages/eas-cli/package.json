{
  "name": "eas-cli",
  "description": "EAS command line tool",
  "version": "0.0.0",
  "author": "Expo <support@expo.io>",
  "bin": {
    "eas": "./bin/run"
  },
  "bugs": "https://github.com/expo/eas-cli/issues",
  "dependencies": {
    "@expo/json-file": "^8.2.22",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "env-paths": "^2.2.0",
    "got": "^11.5.2",
    "prompts": "^2.3.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@types/prompts": "^2.0.8",
    "@vercel/ncc": "^0.23.0",
    "ts-node": "^8",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/expo/eas-cli",
  "keywords": [
    "expo",
    "eas",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "eas",
    "commands": "./lib/commands",
    "plugins": [
      "@eas/plugin-builds",
      "@eas/plugin-updates",
      "@eas/plugin-webhooks",
      "@oclif/plugin-help"
    ],
    "topics": {
      "build": {
        "description": "build app binaries"
      },
      "update": {
        "description": "roll out changes"
      },
      "webhook": {
        "description": "manage webhooks"
      }
    }
  },
  "repository": "expo/eas-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
