          "MEDICAL",
          "MAGAZINES_AND_NEWSPAPERS",
          "UTILITIES",
          "SHOPPING",
          "PRODUCTIVITY",
          "NEWS",
          "PHOTO_AND_VIDEO",
          "NAVIGATION"
        ]
      },
      "AppleSubcategory": {
        "enum": [
          "STICKERS_PLACES_AND_OBJECTS",
