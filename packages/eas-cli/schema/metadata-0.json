                  {
                    "enum": [
                      "STICKERS_PLACES_AND_OBJECTS",
                      "STICKERS_EMOJI_AND_EXPRESSIONS",
                      "STICKERS_CELEBRATIONS",
                      "STICKERS_CELEBRITIES",
                      "STICKERS_MOVIES_AND_TV",
                      "STICKERS_SPORTS_AND_ACTIVITIES",
                      "STICKERS_EATING_AND_DRINKING",
