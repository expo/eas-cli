    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "20.14.2",
    "@types/node-fetch": "^2.6.11",
    "@types/node-forge": "^1.3.11",
    "@types/plist": "^3.0.5",
    "@types/promise-retry": "^1.1.6",
    "@types/retry": "^0.12.5",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "bunyan": "^1.8.15",
    "jest": "^29.7.0",
    "memfs": "^4.17.1",
    "ts-jest": "^29.1.4",
    "ts-mockito": "^2.6.1",
