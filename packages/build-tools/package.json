{
  "name": "@expo/build-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/eas-cli.git",
    "directory": "packages/build-tools"
  },
  "version": "17.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "resources",
    "templates",
    "bin"
  ],
  "scripts": {
    "start": "yarn watch",
    "prewatch": "yarn gql",
    "watch": "tsc --watch --preserveWatchOutput",
    "prebuild": "yarn gql",
    "build": "tsc",
    "typecheck": "tsc",
    "prepack": "rm -rf dist && tsc -p tsconfig.build.json",
    "jest-unit": "jest --config jest/unit-config.ts",
    "jest-integration": "jest --config jest/integration-config.ts",
    "jest-unit-watch": "jest --config jest/unit-config.ts --watch",
    "jest-integration-watch": "jest --config jest/integration-config.ts --watch",
    "clean": "rm -rf node_modules dist coverage *.tsbuildinfo",
    "gql": "gql.tada generate-schema ${API_SERVER_URL:-https://api.expo.dev}/graphql --output ./schema.graphql && gql.tada generate-output",
    "gql:local": "API_SERVER_URL=http://api.expo.test yarn gql",
    "test": "yarn jest-unit"
  },
  "author": "Expo <support@expo.io>",
  "bugs": "https://github.com/expo/eas-cli/issues",
  "license": "BUSL-1.1",
  "dependencies": {
    "@expo/config": "10.0.6",
    "@expo/config-plugins": "9.0.12",
    "@expo/downloader": "17.0.0",
    "@expo/eas-build-job": "17.0.0",
    "@expo/env": "^0.4.0",
    "@expo/logger": "17.0.0",
    "@expo/package-manager": "1.9.10",
    "@expo/plist": "^0.2.0",
    "@expo/results": "^1.0.0",
    "@expo/spawn-async": "1.7.2",
    "@expo/steps": "17.0.0",
    "@expo/template-file": "17.0.0",
    "@expo/turtle-spawn": "17.0.0",
    "@expo/xcpretty": "^4.3.1",
    "@google-cloud/storage": "^7.11.2",
    "@urql/core": "^6.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gql.tada": "^1.8.13",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "node-forge": "^1.3.1",
    "nullthrows": "^1.1.1",
    "plist": "^3.1.0",
    "promise-limit": "^2.7.0",
    "promise-retry": "^2.0.1",
    "resolve-from": "^5.0.0",
    "retry": "^0.13.1",
    "semver": "^7.6.2",
    "tar": "7.5.7",
    "uuid": "9.0.1",
    "yaml": "^2.8.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@expo/repack-app": "~0.2.5",
